/* miniprogram/pages/setts.wxss */
page {
    height: 100%;
    background: #f6f6f6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.c-buttons{
    width: 100%;
    height: 5vh;
    /*border-top: 1px solid #CCC;*/
    /*border-bottom: 1px solid #CCC;*/
    overflow: scroll;
    display: flex;
}
.c-b-node{
    float: left;
    height: 100%;
    padding: 0 5px;
    color: green;
    /*border-left: 1px solid #CCC;*/
}

.c-fs,.c-conter{
    height: 30%;
    background:#ccc;
    overflow: auto;
    padding-bottom: 5px;
}
.c-child{
    padding-left: 5px;
}

.c-conter{
    margin-top: 5px;
    height: calc(100% - 5vh - 30% - 5px);
}