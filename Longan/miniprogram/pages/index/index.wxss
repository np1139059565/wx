page{
  /*background: #000;*/
  /*color: #FFFFFF;*/
  color: #555;
}
.c-head{
  width: 100vw;
  height: 30px;
  line-height: 30px;
  text-align: center;
}
.c-h-button{
  float: left;
  padding: 0 5vw;
}
.c-sv{
  width: 100vw;
  height: calc(100vh - 30px);
}
.c-sv-v{
  width: 100%;
  height: 100%;
}

.c-sv-v>image,.c-sv-v>view{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  background: #f6f6f6;
}
.c-sv-v>image{
  z-index: 2;
}
.c-fill-center1{
  width: 100%;
  height: 30px;
  text-align: center;
}
.c-fill-center2{
  text-align: center;
  display: inline-block;
  vertical-align: top;
}
.c-f-input{
  border-bottom: 1px solid;
  text-align: center;
  width: 4vw;
  margin-left: 1px;
}
.c-answer{
  width: 100%;
  height: calc(34% - 30px);
  text-align: center;
}

.c-choice{
  width: calc(50% - 5vh);
  height: calc(31% - 2vh);
  line-height: calc(31% - 30px - 2vh);
  margin-left: 3vw;
  margin-top: 2vh;
  float: left;
  border-top: 1px solid #888888;
  border-left: 1px solid #888888;
  border-radius: 1vh;
  box-shadow: 2vw 2vw 1vw #888888;
  text-align: center;
}